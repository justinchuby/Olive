{
    "input_model":{
        "type": "PyTorchModel",
        "config": {
            "model_script": "user_script.py",
            "hf_config": {
                "components": [
                    {
                        "name": "decoder_model",
                        "io_config": {
                            "input_names": [ "x", "attn_mask", "k_cache", "v_cache" ],
                            "output_names": [ "logits", "attn_mask_out", "k_out", "v_out", "cos_out", "sin_out"],
                            "dynamic_axes": {
                                "x": { "1": "seq_len" },
                                "attn_mask": { "1": "max_seq_len", "2": "max_seq_len" },
                                "k_cache": { "2": "max_seq_len" },
                                "v_cache": { "2": "max_seq_len" }
                            }
                        },
                        "component_func": "load_decoder_model",
                        "dummy_inputs_func": "decoder_inputs"
                    },
                    {
                        "name": "decoder_with_past_model",
                        "io_config": {
                            "input_names": [ "x_increment", "attn_mask", "k_cache", "v_cache", "cos", "sin" ],
                            "output_names": [ "logits", "attn_mask_out", "k_out", "v_out", "cos_out", "sin_out"],
                            "dynamic_axes": {
                                "attn_mask": { "1": "max_seq_len", "2": "max_seq_len" },
                                "k_cache": { "2": "max_seq_len" },
                                "v_cache": { "2": "max_seq_len" },
                                "cos": { "1": "max_seq_len" },
                                "sin": { "1": "max_seq_len" }
                            }
                        },
                        "component_func": "load_decoder_with_past_model",
                        "dummy_inputs_func": "decoder_with_past_inputs"
                    }
                ]
            }
        }
    },
    "systems": {
        "local_system": {
            "type": "LocalSystem",
            "config": {
                "accelerators": ["gpu"]
            }
        }
    },
    "evaluators": {
        "common_evaluator": {
            "metrics": [
                {
                    "name": "latency",
                    "type": "latency",
                    "sub_types": [{"name": "avg"}],
                    "user_config": {
                        "user_script": "user_script.py",
                        "dataloader_func": "merged_decoders_data_loader",
                        "batch_size": 1
                    }
                }
            ]
        }
    },
    "passes": {
        "convert": {
            "type": "OnnxConversion",
            "config": {
                "target_opset": 17
            }
        },
        "optimize": {
            "type": "OrtTransformersOptimization",
            "disable_search": true,
            "config": {
                "model_type": "unet",
                "float16": true,
                "use_gpu": true,
                "keep_io_types": false,
                "optimization_options": {
                    "enable_gelu": false,
                    "enable_layer_norm": false,
                    "enable_attention": false,
                    "use_multi_head_attention": false,
                    "enable_skip_layer_norm": false,
                    "enable_embed_layer_norm": false,
                    "enable_bias_skip_layer_norm": false,
                    "enable_bias_gelu": false,
                    "enable_gelu_approximation": false,
                    "enable_qordered_matmul": false,
                    "enable_shape_inference": false,
                    "enable_gemm_fast_gelu": false,
                    "enable_nhwc_conv": false,
                    "enable_group_norm": false,
                    "enable_bias_splitgelu": false,
                    "enable_packed_qkv": false,
                    "enable_packed_kv": false,
